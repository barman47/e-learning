<div id="studentLoginModal" class="modal">
    <div class="modal-content">
        <form name="studentLoginForm" method="POST" action="/students/login">
            <div class="row">
                <div class="col s12 center-align">
                    <i class="mdi mdi-account-multiple login-form-avatar"></i>
                    <h4>Student Login</h4>
                </div>
                <div class="col s12">
                    <div class="input-field">
                        <i class="mdi mdi-email-outline prefix form-icons"></i>
                        <input type="email" id="studentEmail" name="studentEmail" class="validate">
                        <label for="studentEmail">Email Address</label>
                        <span id="studentEmailErrorMessage" class="helper-text" data-error="Invalid email address!" data-success=""></span>
                    </div>
                </div>
            
                <div class="col s12">
                    <div class="input-field">
                        <i class="mdi mdi-lock-outline prefix form-icons"></i>
                        <input type="password" id="studentPassword" name="studentPassword" class="validate">
                        <label for="studentPassword">Password</label>
                        <span id="studentPasswordErrorMessage" class="helper-text" data-error="Please provide your password!" data-success=""></span>
                    </div>
                </div>
                {{!-- <span id="incorrectStudentData" class="red-text">Incorrect Email or Password!</span> --}}
            </div>
            <div class="center-align input-field">
                <button id="studentLoginButton" type="submit">Log in</button>
            </div>
            <br>
            <div class="input-field center-align">
                <p>Not Registered? <a class="form-links" href="/students/register">Sign up!</a></p>
            </div>
        </form>
    </div>
</div>

